        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AI_In_Robotics"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="885.687">
    <Grid KeyUp="KeyEvent">
        <Border  Height="400" Width="400" Margin="5,67,474.2,3.4" BorderBrush="Black" BorderThickness="1">
            <Image Name="Image" Height="400" Width="400" RenderTransformOrigin="0.5,0.5" Loaded="OnLoaded" Margin="5,67,474.2,3.4">
                <Image.LayoutTransform>
                    <ScaleTransform ScaleY="-1"></ScaleTransform>
                </Image.LayoutTransform>
            </Image>
        </Border>

        <Border  Height="400" Width="400" Margin="410,67,69.2,3.4" BorderBrush="Black" BorderThickness="1">
            <TextBox x:Name="textBox" TextWrapping="Wrap" Text="TextBox"/>
        </Border>
    </Grid>
</Window>
